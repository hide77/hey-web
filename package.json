{
  "name": "hey-web",
  "version": "0.1.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "build": "webpack --config webpack.config.prod.js",
    "build_staging": "webpack --config webpack.config.dev.js",
    "dev": "webpack-dev-server --config webpack.config.dev.js",
    "dev:hot": "npm run dev -- --hot",
    "docker:dev": "node docker/dev.js",
    "test": "jest",
    "lint": "eslint ./",
    "start": "node server/",
    "clear_toxic_files": "find . -name '.DS_Store' -type f -delete",
    "cpS3": "aws s3 cp dist/ s3://www.hey.network --recursive --profile hey",
    "cpS3_staging": "aws s3 cp dist.dev/ s3://staging.hey.co --recursive --profile hey",
    "invalidateCloudfront": "aws cloudfront create-invalidation --distribution-id E2T6P4HG5HM3J0 --paths '/*' --profile hey",
    "invalidateCloudfront_staging": "aws cloudfront create-invalidation --distribution-id E2VK3VUIMG01W3 --paths '/*' --profile hey",
    "preproduction": "npm run clear_toxic_files && npm run build",
    "production": "npm run cpS3",
    "postproduction": "npm run invalidateCloudfront",
    "prestaging": "npm run clear_toxic_files && npm run build_staging",
    "staging": "npm run cpS3_staging",
    "poststaging": "npm run invalidateCloudfront_staging",
    "deploy": "npm run staging && npm run production"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged && npm run test"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://gitlab.com/teamhey/hey-web.git"
  },
  "dependencies": {
    "babel-polyfill": "^6.26.0",
    "dayjs": "^1.8.17",
    "express": "^4.17.1",
    "express-static-gzip": "^2.0.5",
    "highlight.js": "^9.16.2",
    "history": "^4.10.1",
    "i18next": "^19.0.1",
    "i18next-browser-languagedetector": "^4.0.1",
    "local-storage": "^2.0.0",
    "prop-types": "^15.7.2",
    "react": "^16.10.2",
    "react-autolink-text2": "^3.2.0",
    "react-contextmenu": "^2.13.0",
    "react-copy-to-clipboard": "^5.0.2",
    "react-dom": "^16.10.2",
    "react-dynamic-font": "^2.0.1",
    "react-hot-loader": "^4.12.15",
    "react-i18next": "^9.0.10",
    "react-markdown": "^4.2.2",
    "react-redux": "^7.1.1",
    "react-router-dom": "^5.1.2",
    "react-stripe-checkout": "^2.6.3",
    "react-stripe-elements": "^6.0.1",
    "redux": "^4.0.4",
    "redux-devtools": "^3.5.0",
    "redux-devtools-dock-monitor": "^1.1.3",
    "redux-devtools-log-monitor": "^1.4.0",
    "redux-thunk": "^2.3.0",
    "semantic-ui-react": "^0.88.1",
    "socket.io-client": "^2.3.0",
    "styled-components": "^4.4.0",
    "uifx": "^2.0.3"
  },
  "devDependencies": {
    "@babel/cli": "^7.6.4",
    "@babel/core": "^7.6.4",
    "@babel/plugin-proposal-class-properties": "^7.5.5",
    "@babel/preset-env": "^7.6.3",
    "@babel/preset-react": "^7.6.3",
    "@svgr/cli": "^4.3.3",
    "@svgr/webpack": "^4.3.3",
    "babel-eslint": "^10.0.3",
    "babel-loader": "^8.0.6",
    "brotli-webpack-plugin": "^1.1.0",
    "compression-webpack-plugin": "^3.0.0",
    "css-loader": "^3.2.0",
    "eslint": "^6.5.1",
    "eslint-plugin-react": "^7.16.0",
    "favicons-webpack-plugin": "^1.0.2",
    "file-loader": "^4.2.0",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^3.0.9",
    "jest": "^24.9.0",
    "prettier": "^1.18.2",
    "pretty-quick": "^2.0.0",
    "style-loader": "^1.0.0",
    "webpack": "^4.41.2",
    "webpack-cli": "^3.3.9",
    "webpack-dev-server": "^3.8.2",
    "webpack-manifest-plugin": "^2.2.0",
    "webpack-merge": "^4.2.2"
  }
}
